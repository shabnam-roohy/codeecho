# Example .env file for development environment
# Copy this to .env and modify values as needed

# Base URL for your application
CYPRESS_baseUrl=http://localhost:3000

# API Configuration
CYPRESS_API_URL=http://localhost:3001/api

# Test Environment
CYPRESS_ENV=development

# Test Credentials (use test accounts only!)
CYPRESS_TEST_EMAIL=testuser@example.com
CYPRESS_TEST_PASSWORD=testpassword123

# Cypress Timeouts (milliseconds)
CYPRESS_defaultCommandTimeout=10000
CYPRESS_requestTimeout=10000
CYPRESS_responseTimeout=10000
CYPRESS_pageLoadTimeout=10000

# Viewport Settings
CYPRESS_viewportWidth=1280
CYPRESS_viewportHeight=720

# Test Settings
CYPRESS_video=true
CYPRESS_screenshotOnRunFailure=true

# Custom Environment Variables
# Add your application-specific variables here